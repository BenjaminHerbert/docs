{
    "app_name": "swarmdocs",
    "services": [
        {
            "service_name": "docs-service",
            "components": [
                {
                    "component_name": "docs-auth",
                    "image": "registry.private.giantswarm.io/docs-auth:0.0.1",
                    "ports": [ "80/tcp" ],
                    "dependencies": [{ "name": "upstream", "port": 8000 }],
                    "domains": { "docs.giantswarm.io": "80" }
                },
                {
                    "component_name": "upstream",
                    "image": "registry.private.giantswarm.io/docs:0.14.4",
                    "ports": [ "8000/tcp" ]
                }
            ]
        }
    ]
}
